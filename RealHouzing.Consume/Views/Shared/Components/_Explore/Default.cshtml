@using RealHouzing.Consume.Models.CategoryModels;
@model List<CategoryListViewModel>
<section class="locations-1 w3services-3">
    <div class="locations py-5">
        <div class="container py-lg-5 py-md-4 py-2">
            <div class="heading text-center mx-auto">
                <h6 class="title-subw3hny mb-1">Kategoriler</h6>
                <h3 class="title-w3l mb-3">Mülk Kategorilerimizi Keşfedin</h3>
            </div>
            <div class="row">
                @foreach (var item in Model)
                {
                    
                
                <div class="col-lg-3 col-6 mt-md-5 mt-4">
                    <div class="w3property-grid">
                        <a href="#property">
                            <div class="box16">
                                <img class="img-fluid" src="@item.ImageURL" style="width:250px; height:200px" alt="">
                                <div class="box-content text-center">
                                    <i class="fas fa-home"></i>
                                    <h3 class="title">@item.CategoryName</h3>
                                </div>
                            </div>
                        </a>

                    </div>
                </div>}
      


            </div>
        </div>
    </div>
</section>